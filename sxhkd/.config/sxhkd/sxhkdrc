super + space
    refbar.sh

super + z
    st pulsemixer

super + t
    telegram-desktop

super + r
    j4-dmenu-desktop

super + b
    firefox

super + shift + t
    translate.sh

super + e
    st ranger

Print
    flameshot gui

super + s
    slock

super + v
    clipmenu

super + shift + v
    clipdel -d ".*"

super + shift + w
    feh --bg-scale --randomize ~/Pictures/Wallpapers

super + shift + Return
    st

XF86AudioMute
    amixer -D pulse sset Master toggle; refbar.sh

{XF86AudioRaiseVolume,XF86AudioLowerVolume}
    amixer -D pulse sset Master 5%{+,-} unmute; refbar.sh

{XF86MonBrightnessUp,XF86MonBrightnessDown}
    xbacklight {-inc,-dec} 10; refbar.sh

{XF86AudioNext,XF86AudioPrev,XF86AudioPlay}
    playerctl {next,previous,play-pause}

