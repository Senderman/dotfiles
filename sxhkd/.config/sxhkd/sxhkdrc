super + space
    refbar.sh

super + z
    pavucontrol

super + t
    telegram-desktop

super + r
    rofi -show drun

super + b
    firefox

super + shift + t
    translate.sh

super + e
    st ranger

Print
    flameshot gui

super + s
    bslock

super + shift + Return
    st

XF86AudioMute
    amixer set Master toggle; refbar.sh

XF86AudioRaiseVolume
    amixer -D pulse sset Master 5%+ unmute; refbar.sh

XF86AudioLowerVolume
    amixer -D pulse sset Master 5%- unmute; refbar.sh

XF86MonBrightnessDown
    xbacklight -dec 10; refbar.sh

XF86MonBrightnessUp
    xbacklight -inc 10; refbar.sh;

XF86AudioNext
    playerctl next

XF86AudioPrev
    playerctl previous

Xf86AudioPlay
    playerctl play-pause
