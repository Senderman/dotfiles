super + space
    refbar.sh

super + z
    pavucontrol

super + t
    telegram-desktop

super + r
    rofi -show drun

super + b
    firefox

super + shift + t
    translate.sh

super + e
    st ranger

Print
    flameshot gui

super + s
    bslock

super + shift + Return
    st

XF86AudioMute
    pactl set-sink-mute @DEFAULT_SINK@ toggle

XF86AudioRaiseVolume
    pactl set-sink-volume @DEFAULT_SINK@ +5%; refbar.sh

XF86AudioLowerVolume
    pactl set-sink-volume @DEFAULT_SINK@ -5%; refbar.sh

XF86MonBrightnessDown
    brightnessctl set -10%; refbar.sh

XF86MonBrightnessUp
    brightnessctl set +10%; refbar.sh;

XF86AudioNext
    playerctl next

XF86AudioPrev
    playerctl previous

XF86AudioPlay
    playerctl play-pause
